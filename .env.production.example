# Production Environment Template
# Copy this file to .env.production and fill in values for your environment.

NODE_ENV=production

# Firebase / GCP project id for the deployment target
FIREBASE_PROJECT_ID=tazco-financial-prod

# Provider selection
# - false: Firestore repositories (production)
# - true: InMemory repositories (local/testing only)
USE_INMEMORY=false

# Outbox sequencing
OUTBOX_SEQUENCE_STRATEGY=repository

# Server
PORT=8080
